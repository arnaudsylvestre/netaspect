<?xml version="1.0"?>

<DocumentationConfiguration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                            xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <InterceptorKinds>
    <InterceptorKind>
      <Name>Method</Name>
      <Configurations>
        <InterceptorKindConfiguration>
          <Parameters>
            <ParameterConfiguration>
              <Name>name</Name>
              <Description>description</Description>
            </ParameterConfiguration>
          </Parameters>
          <Interceptors>
            <InterceptorConfiguration>
              <MethodName>Method</MethodName>
              <When>Hello</When>
            </InterceptorConfiguration>
          </Interceptors>
          <Title>Title</Title>
        </InterceptorKindConfiguration>
      </Configurations>
    </InterceptorKind>
  </InterceptorKinds>
</DocumentationConfiguration>
