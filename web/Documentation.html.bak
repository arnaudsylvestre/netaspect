		this.value = intValue;
	}

	public int Value {
		get {
			return value;
		}
	}

	public int DivideBy (int v)
	{
		return value / v;
	}
}


// The code which will call our aspect
{
	try {
		new MyInt (0);
		Assert.Fail ("Must raise an exception");
	}
	catch (NotSupportedException) {
	}
	Assert.True (LogAttribute.Called);
}
