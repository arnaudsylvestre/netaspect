		Assert.AreEqual (2, result);
	}
}


// The class to weave
public class MyInt
{
	int value;

	public MyInt (int value)
	{
		this.value = value;
	}

	public int Value {
		get {
			return value;
		}
	}

	[Log]
	public int DivideBy (int v)
	{
		return value / v;
	}
}


// The code which will call our aspect
{
	var myInt = new MyInt (24);
	Assert.AreEqual (2, myInt.DivideBy (12));
	Assert.True (LogAttribute.Called);
